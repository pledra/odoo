<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <record id="digest_view_form_inherit_website_sale_order" model="ir.ui.view">
    <field name="name">digest.view.form.inherit.website.sale.order</field>
    <field name="model">digest.digest</field>
    <field name="inherit_id" ref="digest.digest_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//group[@name='kpi_sales']" position="attributes">
        <attribute name="string">Sales &amp; eCommerce</attribute>
      </xpath>
      <xpath expr="//group[@name='kpi_sales']" position="inside">
        <field name="kpi_website_sale_total"/>
      </xpath>
    </field>
  </record>
</odoo>
